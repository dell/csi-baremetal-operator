{
    "categories": [
        {
            "title": "## üöÄ Enhancements",
            "labels": [
                "enhancement"
            ]
        },
        {
            "title": "## üêõ Bugs",
            "labels": [
                "bug"
            ]
        },
        {
            "title": "## üìô Documentation",
            "labels": [
                "documentation"
            ]
        }
    ],
    "sort": "ASC",
    "template": "${{CHANGELOG}}\n\n<details>\n<summary>Uncategorized</summary>\n\n${{UNCATEGORIZED}}\n</details>",
    "pr_template": "- ${{TITLE}}\n   - PR: #${{NUMBER}}",
    "empty_template": "- no changes",
    "label_extractor": [
        {
            "pattern": "(.) (.+)",
            "target": "$1",
            "flags": "gu"
        },
        {
            "pattern": "\\[Issue\\]",
            "on_property": "title",
            "method": "match"
        }
    ],
    "duplicate_filter": {
        "pattern": "\\[ABC-....\\]",
        "on_property": "title",
        "method": "match"
    },
    "transformers": [
        {
            "pattern": "[\\-\\*] (\\[(...|TEST|CI|SKIP)\\])( )?(.+?)\n(.+?[\\-\\*] )(.+)",
            "target": "- $4\n  - $6"
        }
    ],
    "max_tags_to_fetch": 200,
    "max_pull_requests": 200,
    "max_back_track_time_days": 365,
    "tag_resolver": {
        "method": "semver"
    },
    "base_branches": [
        "master"
    ]
}